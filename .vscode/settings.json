    "editor.tabSize": 4
  },
  "editor.insertSpaces": true,
  "liveSassCompile.settings.formats": [
    {
      "extensionName": ".css",
      "format": "expanded",
      "savePath": "/css"
    }
  ],
  "javascript.preferences.quoteStyle": "double"
}